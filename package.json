{
  "private": false,
  "name": "style-wiz",
  "description": "Powerful CSS-in-JS and build-time stylesheets tool designed to simplify CSS-in-JS Developer experience and streamline the process of generating minified CSS files at build-time tailored for your needs.",
  "version": "0.2.0",
  "main": "./build/index.js",
  "module": "./build/index.cjs.js",
  "typings": "./build/src/index.d.ts",
  "exports": {
    "require": "./build/index.cjs.js",
    "import": "./build/index.js"
  },
  "files": [
    "build/**/*"
  ],
  "types": "build/index.d.ts",
  "bin": "src/bin/generate-stylesheets.mjs",
  "scripts": {
    "test": "jest",
    "build": "rm -rf build && rollup -c",
    "lint": "eslint ./src --ext .ts,.tsx --config .eslintrc --fix",
    "format": "prettier --write .",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "styled-components",
    "npm",
    "package",
    "typescript",
    "npm package"
  ],
  "author": "Milad Afkhami",
  "license": "MIT",
  "peerDependencies": {
    "goober": "^2.1.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@rollup/plugin-typescript": "^11.1.2",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.195",
    "@types/react": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "goober": "^2.1.13",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "prettier": "2.8.8",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.2",
    "ts-jest": "29.1.1",
    "tslib": "2.6.0",
    "typescript": "5.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milad-afkhami/style-wiz.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false
}
